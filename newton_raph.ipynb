    "def f(x):\n",
    "    return x*x*x-x-1\n",
    "def f1(x):\n",
    "    return 3*x*x-1\n",
    "def newton_raph(x):\n",
    "    h=f(x)/f1(x)\n",
    "    while(abs(h)>=0.00001):\n",
    "        h=f(x)/f1(x)\n",
    "        x=x-h\n",
    "        print(\"The value of root is : \", \"%f\" %x)\n",
    "        \n",
    "x0=1\n",
    "newton_raph(x0)\n",
    "    "
   ]
